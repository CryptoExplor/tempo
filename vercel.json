{
  "version": 2,
  "routes": [
    {
      "src": "/js/config.js",
      "dest": "/js/config.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/js/app.js",
      "dest": "/js/app.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/js/ui.js",
      "dest": "/js/ui.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/js/features/(.*).js",
      "dest": "/js/features/$1.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/icon.png",
      "dest": "/icon.png",
      "headers": {
        "Content-Type": "image/png"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/js/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/js/:path*",
      "destination": "/js/:path*"
    }
  ]
}
